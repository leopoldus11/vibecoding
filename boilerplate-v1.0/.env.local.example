# === FRONTEND CONFIG ===
VITE_SITE_URL=http://localhost:5173

# === SUPABASE (Database & Edge Functions) ===
# Found in Project Settings -> API
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key
# Used ONLY in Edge Functions (Managed in Supabase Secrets)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# === RESEND (Email Engine) ===
# Found in Resend Dashboard -> API Keys
# Re-create and save in Supabase Secrets as RESEND_API_KEY
RESEND_API_KEY=re_your_api_key

# === PAYPAL (Payments) ===
# Found in PayPal Developer Dashboard -> Apps & Credentials
VITE_PAYPAL_CLIENT_ID=your-paypal-client-id

# === ANALYTICS & DATA ===
# Verification IDs or API keys
VITE_GA_MEASUREMENT_ID=G-XXXXXXX
VITE_BIGQUERY_PROJECT_ID=your-project-id

# === CALENDAR (Scheduling) ===
# Personal link or API booking slug
VITE_CAL_LINK=yourname/vibe-session
